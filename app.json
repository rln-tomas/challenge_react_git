{
  "expo": {
    "name": "Fravega Challenge React Native",
    "slug": "challenge_react_git",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fravega_icon.png",
    "scheme": "challengereactgit",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/fravega_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.challenge-react-git",
      "splash": {
        "image": "./assets/images/fravega_icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/fravega_icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.challenge_react_git"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
